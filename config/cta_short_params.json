{
    "input_data": {
        "dirpath": ["mktdata","binance","downloads"],
        "start_date": "2022-08-15T00:00:00",
        "selected_instruments": [
            "AAVEUSDT",
            "ADAUSDT",
            "AVAXUSDT",
            "BNBUSDT",
            "BTCUSDT",
            "DOGEUSDT",
            "DOTUSDT",
            "ETHUSDT",
            "MATICUSDT",
            "XRPUSDT"
        ]
    },
    "run_parameters": {
        "pickle_file": "engine.pickle",
        "unit_test": false,
        "groupby": ["hvol_close"],
        "cross_ewma": false,
        "tx_cost": 0,
        "delta_strategy": {"type": "threshold_leverage", "params": {
            "expectation_threshold": 1, "max_leverage": 20}
        },
        "models": {
            "quantized_zscore": {
                "RandomForestClassifier": {"splits_for_1000": 1,
                    "params": {"min_samples_split": 10,
                    "n_jobs": -1,
                    "warm_start": false}}
            }
        }
    },
    "feature_map": {
        "close": {
            "ewma_expansion": {
                "cross_ewma": false,
                "weight_by": "volume",
                "windows":[1, 5, 60, 1440, 4320]},
            "max_expansion": {
                "windows": [5, 60, 1440, 4320]},
            "min_expansion": {
                "windows": [5, 60, 1440, 4320]},
            "hvol_expansion": {
                "incr_window": [[1,60]],
                "quantiles": [0.35, 0.65, 1.0]}
        },
        "volume": {
            "transform": "log",
            "ewma_expansion":  {
                "windows": [60],
                "quantiles": [0.35, 0.65, 1.0]}
        },
        "taker_buy_volume": {
            "transform": "arctanh"},
        "close_premium": {
            "ewma_expansion":  {
                "weight_by": "volume",
                "windows": [60],
                "quantiles": [0.35, 0.65, 1.0]}
        }
    },
    "label_map": {
        "quantized_zscore": {"horizons": [60], "stdev_quantum":  1.5, "stdev_cap": 3}
    }
}